long double solve(double number, int n){
    long double ans = 1.0;
    for(int i=1;i<=n;i++){
        ans = ans*number;
    }
    return ans;
}

double findNthRootOfM(int n, long long m) {
	// Write your code here.
    long double low = 1;
    long double high = m+1;
    long double esp = 1e-9;
    while((high-low) > esp){
        long double mid = (low+high)/2.0;
        if(solve(mid,n) < (double)m){
            low=mid;
        }
        else{
            high = mid;
        }
    }
    return low;
}
